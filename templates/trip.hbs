<h3>{{name}}</h3>
<button class="btn our-btn" {{action "startList"}}>Group Camping List</button>
{{#if list}}
 <h4>Select Group Gear</h4>
{{#each item in campingListItems }}
  {{input type="checkbox" checked=item.need}}
  {{item.name}}
{{/each}}
<button type="submit" {{action "addCampItems"}}>Add To Your List</button>
{{/if}}
